		function ActiveXInspection(ProgId){
		  var isInstall = false;
		  try {
		    var obj = new ActiveXObject(ProgId);
		    if (obj) return true;
		  } catch (e) {
		    return false;
		  }

		  return false;
		}



		function ActiveXInspectionInnerHtml(class_id,ProgId,down_url){

			var isInstall=ActiveXInspection(ProgId);
			var exp_url;
			if(isInstall){

			 exp_url="<p>설치완료</p>";

			}else{
				 exp_url="<p><font color='red'>미설치</font></p>";

			}

		$('#'+class_id).html(exp_url);

		return exp_url ;
		}







